        "@vue/compiler-sfc": "^3.2.19",
        "autoprefixer": "10.3.6",
        "highlight.js": "11.3.1",
        "mitt": "^3.0.0",
        "moment": "^2.29.1",
        "node-sass": "^6.0",
        "npm-check-updates": "^11.8.5",
        "rollup": "2.58.0",
        "rollup-plugin-copy": "3.4.0",
